    { "name":"Number","scope":["constant.numeric","constant.language.boolean"],"settings":{"foreground":"#FFC107"} },
    { "name":"Keyword & Storage","scope":["keyword","storage.type","storage.modifier"],"settings":{"foreground":"#00D8FF"} },
